$('#message_group_back_create').parent().remove();
if (<%= @message_group.new? ? 1 : 0 %> > 0) {
  $("#message_group_create").hide().after('<%= j render("create_form") %>');
};

if (<%= @message_group.persisted? %>) {
  $("#message_group_create").remove();
  $(".comments-box").prepend('<%= j render("edit_forma") %>');
};
